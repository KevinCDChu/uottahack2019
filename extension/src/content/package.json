{
  "name": "content_script",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "react-scripts": "1.1.4",
    "remote-redux-devtools": "^0.5.12",
    "redux-test-utils": "^0.2.2",
    "source-map-explorer": "^1.4.0"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-chrome-redux": "^1.5.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && npm run build-css -- --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p rename-local watch-css start-js",
    "rename-local": "cd src && cp index.local.js index.js",
    "rename-extension": "cd src && cp index.extension.js index.js",
    "build-js": "react-scripts build",
    "build": "npm-run-all rename-extension build-css build-js move-css move-js remove-redundant-build-file",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint-check": "eslint -c .eslintrc.json ./*",
    "move-css": "cd build/static/css && mv main.*.css ../../index.css && mv main.*.css.map ../../index.css.map",
    "move-js": "cd build/static/js && mv main.*.js ../../index.js && mv main.*.js.map ../../index.js.map",
    "remove-redundant-build-file": "cd build && rm -rf static && rm *.json && rm *.html && rm service-worker.js"
  }
}
